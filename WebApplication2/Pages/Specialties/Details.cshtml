@page
@model WebApplication2.Pages.Specialties.DetailsModel
@{
    ViewData["Title"] = "Данные о "+Model.Specialty.CodeSpecialty;
}

<div>
    <h4>Специальность</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Код направления
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Specialty.CodeSpecialty)
        </dd>
        <dt class="col-sm-2">
            Название направления
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Specialty.NameSpecialty)
        </dd>
        <dt class="col-sm-2">
            Профиль направления
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Specialty.TrainingSpecialty)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Specialty?.CodeSpecialty">Изменить</a> |
    <a asp-page="./Index">Вернуться к списку</a>
</div>
